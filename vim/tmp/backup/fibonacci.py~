from numpy import *

def calculateFobinacci (inData):
   dim = int(inData)
   base = matrix([(1, 1), (1, 0)])
   return (base **  dim) [0, 0];

if __name__ == "__main__":
    import sys
    fib(int(sys.argv[1]))

